{
	"name": "radionovel/laravel-kafka",
	"description": "Laravel Kafka",
	"type": "library",
	"keywords": [],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Andrey Radionov",
			"email": "andrey.radionoff@gmail.com"
		}
	],
	"support": {
		"issues": "https://github.com/radionovel/laravel-kafka/issues?state=open",
		"source": "https://github.com/radionovel/laravel-kafka"
	},
	"require": {
		"php": ">=8.1",
		"ext-rdkafka": "*",
		"illuminate/console": ">=9.0.0",
		"illuminate/container": ">=9.0.0",
		"illuminate/database": ">=9.0.0",
		"illuminate/support": ">=v9.0.0",
		"ramsey/uuid": "^3.9"
	},
	"require-dev": {
        "phpunit/phpunit": "^9.5.25"
	},
	"autoload": {
		"psr-4": {
			"RlKafka\\": "src/"
		}
	},
    "extra": {
        "laravel": {
            "providers": [
                "RlKafka\\Providers\\RlKafkaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
